        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:layout_editor_absoluteY="136dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="number"
